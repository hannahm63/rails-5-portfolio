<main role="main" class="inner cover app-container">
  <h1 class="col-md-12 cover-heading">Tech News</h1>

  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card-body">
            <p class="card-text">
              <%= tweet %>
            </p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</main>
